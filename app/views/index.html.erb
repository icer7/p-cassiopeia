<p id="notice"><%= notice %></p>

<h1>cassiopeia</h1>

<table>

  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <button onclick="startVideo()">ビデオ開始</button>
    <button onclick="stopVideo()">ビデオ終了</button>
    <button onclick="snapshot()">写真撮影</button>
    <button onclick="reset()">写真削除</button>
    <button onclick="submit()">計測開始</button>

    <div class="video">
      <h2>ビデオ撮影</h2>
        <p>
          <video autoplay width="320" height="240"></video>
        </p>
    </div>

    <div class="snapshot">
      <h2>撮影写真</h2>
        <p>
          <img/>
          <canvas id="canvas" style="display: none" width="320" height="240"></canvas>
        </p>
    </div>
  </tbody>

</table>

<br>
